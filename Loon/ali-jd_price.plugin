[Script]
#阿里云社区Cookie
http-response ^https?:\/\/developer\.aliyun\.com\/developer\/api\/my\/user\/getUser script-path=https://raw.githubusercontent.com/leiyiyan/resource/main/script/aliyun_web/aliyun_web.js, requires-body=true, timeout=60, tag=阿里云社区Cookie, img-url=https://raw.githubusercontent.com/Sliverkiss/QuantumultX/refs/heads/main/icon/aliyunWeb.png
#阿里云社区签到
cron "0 9,10,15 * * *" script-path=https://raw.githubusercontent.com/leiyiyan/resource/main/script/aliyun_web/aliyun_web.js, tag=阿里云社区, img-url=https://raw.githubusercontent.com/Sliverkiss/QuantumultX/refs/heads/main/icon/aliyunWeb.png

#京东比价弹窗
http-request ^https:\/\/in\.m\.jd\.com\/product\/graphext\/\d+\.html script-path=https://raw.githubusercontent.com/mw418/Loon/main/script/jd_price.js, requires-body=true, timeout=60, tag=京东比价弹窗, img-url=https://raw.githubusercontent.com/Former-Years/icon/master/jd.png

[MITM]
hostname = developer.aliyun.com, in.m.jd.com
